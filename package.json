  "main": "public/electron.js",
  "homepage": "./",
  "copyright": "Copyright ⓒ 2024. THE NEXT inc. all rights reserved.",
  "author": "THE NEXT TEAM",
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@iconify/types": "^2.0.0",
    "@tanstack/react-table": "^8.7.9",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@tippyjs/react": "^4.2.6",
    "@types/cleave.js": "^1.4.12",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-collapse": "^5.0.4",
    "@types/react-dom": "^18.0.0",
    "@types/react-icons": "^3.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/recoil": "^0.0.9",
    "apexcharts": "^3.47.0",
    "cleave.js": "^1.6.0",
    "dayjs": "^1.11.10",
    "electron-is-dev": "^3.0.1",
    "electron-log": "^5.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.1.7",
    "framer-motion": "^11.0.6",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-calendar": "^4.8.0",
    "react-collapse": "^5.1.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.21.3",
    "react-scripts": "5.0.1",
    "react-tailwindcss-datepicker": "^1.6.6",
    "recoil": "^0.7.7",
    "sass": "^1.71.1",
    "simplebar-react": "^3.2.4",
    "swiper": "^8.4.5",
    "typescript": "^5.3.3",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@iconify/react": "^4.1.1",
    "@storybook/addon-essentials": "^7.6.14",
    "@storybook/addon-interactions": "^7.6.14",
    "@storybook/addon-links": "^7.6.14",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/addon-storysource": "^7.6.17",
    "@storybook/blocks": "^7.6.14",
    "@storybook/cli": "^7.6.14",
    "@storybook/preset-create-react-app": "^7.6.14",
    "@storybook/react": "^7.6.14",
    "@storybook/react-webpack5": "^7.6.14",
    "@storybook/test": "^7.6.14",
    "chromatic": "^10.9.4",
    "concurrently": "^8.2.2",
    "electron": "^28.1.3",
    "electron-builder": "^24.9.1",
    "eslint-plugin-storybook": "^0.6.15",
    "gh-pages": "^6.1.1",
    "prop-types": "^15.8.1",
    "storybook": "^7.6.14",
    "tailwindcss": "^3.4.1",
    "wait-on": "^7.2.0",
    "webpack": "^5.90.1"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:n2soft-application/electron-client.git",
    "release": "latest"
  },
  "build": {
    "appId": "kr.co.bonjin.electron",
    "artifactName": "${productName}-${version}.${ext}",
    "publish": {
      "provider": "github"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "identity": null
    },
    "dmg": {
      "title": "리테일 금융 시스템",
      "sign": false,
      "icon": "./src/assets/icons/icon.ico"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "verifyUpdateCodeSignature": false,
      "icon": "./src/assets/icons/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "language": 1042,
      "shortcutName": "리테일 금융 시스템",
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "zip",
        "tar.gz"
      ],
      "icon": "./resources/linuxicon"
    },
    "directories": {
      "output": "dist"
    }
  },
  "scripts": {
    "start": "concurrently \"yarn react:start\" \"yarn electron:start\"",
    "electron:start": "wait-on tcp:127.0.0.1:3000 && mode=dev electron .",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "dist": "electron-builder",
    "dist:mac": "electron-builder --mac",
    "dist:mac64": "electron-builder -m --x64 --arm64 --publish always",
    "dist:win32": "electron-builder --win --ia32",
    "dist:win64": "electron-builder --win --x64",
    "dist:win": "CI=false react-scripts build && electron-builder --win --publish always",
    "build:start": "electron-forge start",
    "build:package": "electron-forge package",
    "build:make": "electron-forge make",
    "storybook": "storybook dev -p 6006",
    "predeploy": "yarn run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=chpt_026f64c6f7d9d96"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
